{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2019",
        "lib": [
            "esnext"
        ],
		"esModuleInterop": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
        "outDir": "dist",
        "sourceMap": true,
        "strict": true
    },
    "exclude": [
        "node_modules"
    ]
}
